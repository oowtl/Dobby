{
  "expo": {
    "name": "Dobby",
    "slug": "Dobby",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/dobby.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "port": 52342
    },
	"ios": {
      "bundleIdentifier": "com.ssafy.dobby",
      "buildNumber": "1.0.0",
	  "supportsTablet": true
    },
    "android": {
      "package": "com.ssafy.dobby",
      "versionCode": 1,
	  "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
	  "permissions": [
		"ACCESS_COARSE_LOCATION",
		"ACCESS_FINE_LOCATION",
		"ACCESS_BACKGROUND_LOCATION"
	  ]
    } 
  }
}
